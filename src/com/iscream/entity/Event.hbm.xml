<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.iscream.entity.Event" table="event" schema="" catalog="testol">
        <id name="id">
            <column name="id" sql-type="int" length="10" not-null="true"/>
            <generator class="native"/>
        </id>
        <property name="date">
            <column name="date" sql-type="timestamp" length="19" not-null="true"/>
        </property>
        <property name="cno">
            <column name="cno" sql-type="varchar" length="16" not-null="true"/>
        </property>
        <property name="operate">
            <column name="operate" sql-type="varchar" length="16" not-null="true"/>
        </property>
        <property name="tqno">
            <column name="tqno" sql-type="int" length="10" not-null="true"/>
        </property>
        <property name="optionA">
            <column name="optionA" sql-type="varchar"/>
        </property>
        <property name="optionB">
            <column name="optionB" sql-type="varchar"/>
        </property>
        <property name="optionC">
            <column name="optionC" sql-type="varchar"/>
        </property>
        <property name="optionD">
            <column name="optionD" sql-type="varchar"/>
        </property>
        <property name="optionE">
            <column name="optionE" sql-type="varchar"/>
        </property>
        <property name="optionF">
            <column name="optionF" sql-type="varchar"/>
        </property>
        <property name="optionG">
            <column name="optionG" sql-type="int" length="10"/>
        </property>
        <property name="score">
            <column name="score" sql-type="int" length="10"/>
        </property>
    </class>
</hibernate-mapping>