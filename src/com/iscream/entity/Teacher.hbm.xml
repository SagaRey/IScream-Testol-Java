<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.iscream.entity.Teacher" table="teacher" schema="" catalog="testol">
        <id name="id">
            <column name="id" sql-type="int" length="10" not-null="true"/>
            <generator class="native"/>
        </id>
        <property name="tid">
            <column name="tid" sql-type="varchar" length="16" not-null="true"/>
        </property>
        <property name="tname">
            <column name="tname" sql-type="varchar" length="32" not-null="true"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar" length="16" not-null="true"/>
        </property>
        <property name="subject">
            <column name="subject" sql-type="varchar" length="64" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>