<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.iscream.entity.Testcard" table="testcard" schema="" catalog="testol">
        <id name="id">
            <column name="id" sql-type="int" length="10" not-null="true"/>
            <generator class="native"/>
        </id>
        <property name="date">
            <column name="date" sql-type="timestamp" length="19" not-null="true"/>
        </property>
        <property name="cno">
            <column name="cno" sql-type="varchar" length="16" not-null="true"/>
        </property>
        <property name="sid">
            <column name="sid" sql-type="varchar" length="16" not-null="true"/>
        </property>
        <property name="tno">
            <column name="tno" sql-type="varchar" length="16" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>